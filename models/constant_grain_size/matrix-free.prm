include S20RTS.prm

set Dimension = 3
#set Additional shared libraries = ../../plugins/libequilibrium_grain_size.so
set Output directory = output-matrix-free

subsection Solver parameters
  subsection Stokes solver parameters
    set Stokes solver type = block GMG
    set Number of cheap Stokes solver steps             = 2000
    set Maximum number of expensive Stokes solver steps = 100
  end
end

subsection Material model
  set Material averaging = harmonic average
end
  
